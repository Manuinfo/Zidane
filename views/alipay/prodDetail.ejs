  <div class="row" style="margin-top: 10px;">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 fixpad prddtlunit">
      <p class="lead"><%=intro_text %></p>
    </div>
  </div>
  <div class="row" style="margin-top: 10px;">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 fixpad">
      <img src="/msite/dtl/<%=intro_img%>.png" width="100%" height="280px" />
